Master {
  akka {
    home = ""
    version = "2.5.22"
    actor {
      provider = "cluster"
    }
    remote {
      maximum-payload-bytes = 30000000 bytes
      log-remote-lifecycle-events = off
      netty.tcp {
        hostname = "127.0.0.1"
        port = 0
        message-frame-size = 30000000b
        send-buffer-size = 30000000b
        receive-buffer-size = 30000000b
        maximum-frame-size = 30000000b
      }
    }

    cluster {
      roles = ["Master"]
      seed-nodes = [
        "akka.tcp://SampleActorSystem@127.0.0.1:2556"
      ]
    }
  }
}

Worker {
  akka {
    version = "2.5.22"

    actor {
      provider = "cluster"
    }
    remote {
      maximum-payload-bytes = 30000000 bytes
      log-remote-lifecycle-events = off
      netty.tcp {
        hostname = "127.0.0.1"
        port = 0
        message-frame-size = 30000000b
        send-buffer-size = 30000000b
        receive-buffer-size = 30000000b
        maximum-frame-size = 30000000b
      }
    }

    cluster {
      roles = ["Worker"]
      seed-nodes = [
        "akka.tcp:SampleActorSystem@127.0.0.1:2556"
      ]
    }
  }
}
